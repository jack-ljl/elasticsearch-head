<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC "-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB//EN" "http://www.bea.com/servers/wls810/dtd/weblogic-ejb-jar.dtd">
<weblogic-ejb-jar>
  <weblogic-enterprise-bean>
    <ejb-name>MissiveAdmin</ejb-name>
    <stateless-session-descriptor>
      <pool/>
      <stateless-clustering/>
    </stateless-session-descriptor>
    <transaction-descriptor/>
    <enable-call-by-reference>True</enable-call-by-reference>
    <jndi-name>ejb.missive.MissiveAdminRemote</jndi-name>
    <local-jndi-name>ejb.missive.MissiveAdmin</local-jndi-name>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>MissiveProcessor</ejb-name>
    <stateless-session-descriptor>
      <pool/>
      <stateless-clustering/>
    </stateless-session-descriptor>
    <transaction-descriptor/>
    <enable-call-by-reference>True</enable-call-by-reference>
    <jndi-name>ejb.missive.MissiveProcessorRemote</jndi-name>
    <local-jndi-name>ejb.missive.MissiveProcessor</local-jndi-name>
  </weblogic-enterprise-bean>
</weblogic-ejb-jar>
